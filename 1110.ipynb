    "n = int(input())\n",
    "check = n\n",
    "cnt = 0\n",
    "\n",
    "while True:\n",
    "    ten = n//10 #2\n",
    "    one = n%10 #6\n",
    "    tmp = ten+one #8\n",
    "    n = 10*one+tmp%10 #68\n",
    "    cnt += 1\n",
    "    \n",
    "    if n == check:\n",
    "        break\n",
    "        \n",
    "print(cnt)\n"
   ]
